---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for page in site.pages %}{% if page.sitemap != null and page.sitemap != empty %}
  <url>
    <loc>{{ site.url }}{{ page.url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>{{ page.sitemap.priority | default: '0.8' }}</priority>
  </url>
{% endif %}{% endfor %}
{% assign event = (site.data.events | where:"future",true | sort:"startTime" | first) %}
{% if event.ticketUrl %}
  <url>
    <loc>{{ event.ticketUrl }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.8</priority>
  </url>
{% endif %}
</urlset>